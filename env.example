# Database Configuration
DATABASE_URL="postgresql://user:password@localhost:5432/anchor_db"

# Firebase Configuration
# Get these values from your Firebase project settings
FIREBASE_API_KEY="your_api_key_here"
FIREBASE_AUTH_DOMAIN="your_project_id.firebaseapp.com"
FIREBASE_PROJECT_ID="your_project_id"
FIREBASE_STORAGE_BUCKET="your_project_id.appspot.com"
FIREBASE_MESSAGING_SENDER_ID="your_sender_id"
FIREBASE_APP_ID="your_app_id"
FIREBASE_MEASUREMENT_ID="G-your_measurement_id"

# Google Sign-In Configuration
# Get these from Firebase Console > Authentication > Sign-in method > Google
GOOGLE_WEB_CLIENT_ID="your_google_web_client_id.apps.googleusercontent.com"

# Cloud SQL Configuration (for Firebase Data Connect)
CLOUD_SQL_CONNECTION_NAME="your_project_id:region:instance_name"
CLOUD_SQL_DATABASE_NAME="anchor_db"
CLOUD_SQL_USER="your_cloud_sql_user"
CLOUD_SQL_PASSWORD="your_cloud_sql_password"
